<?xml version="1.0" encoding="utf-8" ?>
<configuration>

    <configSections>
        <section name="easyNetQHost" type="EasyNetQ.Host.Service.Configuration.HostSection, EasyNetQ.Host.Service"/>
    </configSections>

    <easyNetQHost>
        <connectionStrings>
            <add name="Broker_A" connectionString="host=localhost"/>
            <add name="Broker_B" connectionString="host=localhost"/>
            <add name="Broker_C" connectionString="host=localhost"/>
        </connectionStrings>
    </easyNetQHost>

    <connectionStrings>
        <add name="rabbit" connectionString="host=localhost"/>
    </connectionStrings>

    <appSettings>
        <add key="RabbitManagementClientHostUrl" value="http://localhost"/>
    </appSettings>
    
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    
</configuration>